@model List<QualifyMeProject.ViewModels.UserViewModel>

@{ 
    ViewBag.Title = "Students";
}

<div class="row">
    <div class="col-sm-12">
        <h4>List of Students</h4>
        <ul class="list-group list-group-flush">
            @foreach (var item in Model)
            {
                <li class="list-group-item list-group-item-action">@item.ID. @item.Name</li>
            }
        </ul>
    </div>
</div>

