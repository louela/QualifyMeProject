@model List<QualifyMeProject.ViewModels.UserViewModel>

@{ 
    ViewBag.Title = "Students";
}

<div class="row">
    <div class="col-sm-12">
        <h4>List of Student</h4>
        <ul class="list-group list-group-flush">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>Student ID</th>
                        <th>Name</th>
                        <th>Mobile</th>
                        <th>Email</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tbody>
                        <tr>
                            <td>@item.ID</td>
                            <td>@item.Name</td>
                            <td>@item.Mobile</td>
                            <td>@item.Email</td>
                        </tr>
                    </tbody>
                }
            </table>
        </ul>
    </div>
</div>

