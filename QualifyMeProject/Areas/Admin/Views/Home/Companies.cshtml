@model List<QualifyMeProject.ViewModels.CompanyUserViewModel>

@{ 
    ViewBag.Title = "Companies";
}

<div class="row">
    <div class="col-sm-12">
        <h4>List of Companies</h4>
        <ul class="list-group list-group-flush">
            <li class="nav-item active">
                <a class="nav-link" href="/admin/companies/addcompany">Add Company</a>
            </li>
            @foreach (var item in Model)
            {
                <li class="list-group-item list-group-item-action">@item.CompanyID. @item.CompanyName</li>
            }
        </ul>
    </div>
</div>

